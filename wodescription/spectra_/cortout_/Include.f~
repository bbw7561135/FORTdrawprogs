      IMPLICIT REAL*8(A-H,L,O-Z), INTEGER*4(I,J,K,M,N)

      PARAMETER (Zero=0.0D0,One=1.0D0,Two=2.0D0,Three=3.0D0)
      PARAMETER (Four=4.0D0,Six=6.0D0,Ten=1.0D1,Cent =1.0D2)
      PARAMETER (Half=One/Two,Quad=One/Four,OS=One/Six)
      PARAMETER (pi=3.141592653589793)
      PARAMETER (Nmax=400, VeryBig=0.99d30)